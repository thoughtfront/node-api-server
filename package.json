{
  "name": "node-api-server",
  "version": "0.0.1",
  "description": "A template node api server",
  "main": "src/index.js",
  "author": "Christopher Dimoulis",
  "license": "MIT",
  "scripts": {
    "build:dev": "webpack --mode development --watch",
    "create:migration": "knex migrate:make",
    "create:seed": "knex seed:make",
    "db:help": "knex --help",
    "db:migrate": "knex migrate:latest",
    "db:seed": "knex seed:run",
    "db:status": "knex migrate:status",
    "db:rollback": "knex migrate:rollback",
    "dev": "nodemon -r dotenv/config dist/index.js",
    "start": "yarn dev"
  },
  "dependencies": {
    "axios": "~0.21.0",
    "body-parser": "~1.19.0",
    "cors": "~2.8.5",
    "express": "~4.17.1",
    "knex": "~0.21.14",
    "pg": "^8.5.1",
    "swagger-ui-express": "~4.1.5",
    "uuid": "~8.3.1"
  },
  "devDependencies": {
    "dotenv": "~8.2.0",
    "nodemon": "~2.0.6",
    "webpack": "~5.10.3",
    "webpack-cli": "~4.2.0",
    "webpack-node-externals": "~2.5.2"
  }
}
